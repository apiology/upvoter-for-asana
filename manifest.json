{
  "name": "Upvoter for Asana",
  "description": "Quickly increment integer custom fields in Asana tasks from the Chrome Omnibox",
  "version": "0.1.0",
  "permissions": [
    "activeTab"
  ],
  "omnibox": { "keyword": "uv" },
  "background": {
    "scripts": ["asana.js", "config.js", "upvoter.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}
